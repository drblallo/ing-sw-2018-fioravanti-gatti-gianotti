<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane fx:id="myPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="progetto.view.gui.GamePaneController">
    <children>
      <VBox spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox>
               <children>
                  <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackButtonClicked" text="Indietro" />
                  <Region HBox.hgrow="ALWAYS" />
               </children>
            </HBox>
            <TabPane fx:id="tabPane" prefHeight="700.0" prefWidth="700.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <tabs>
                  <Tab fx:id="otherPlayers" text="Other Players">
                     <content>
                        <VBox fx:id="mainVBox" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                           <children>
                              <HBox fx:id="hBox" />
                              <VBox fx:id="preGameVBox">
                                 <children>
                                    <fx:include fx:id="preGamePane" source="PreGamePane.fxml" />
                                 </children>
                              </VBox>
                              <VBox fx:id="roundVBox">
                                 <children>
                                    <fx:include fx:id="mainBoardPane" source="MainBoardPane.fxml" />
                                    <HBox fx:id="roundMenuHBox">
                                       <children>
                                          <Region HBox.hgrow="ALWAYS" />
                                          <fx:include fx:id="roundTrackPane" source="RoundTrackPane.fxml" />
                                          <Region HBox.hgrow="ALWAYS" />
                                          <fx:include fx:id="playerMenuPane" source="PlayerMenuPane.fxml" />
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                           <padding>
                              <Insets left="10.0" right="10.0" />
                           </padding>
                        </VBox>
                     </content>
                  </Tab>
                <Tab text="Action Queue ">
                     <content>
                        <fx:include fx:id="actionQueuePane" source="ActionQueuePane.fxml" />
                     </content>
                </Tab>
                  <Tab fx:id="commandLine" text="Command Line">
                    <content>
                      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <fx:include fx:id="commandLinePane" source="CommandLinePane.fxml" />
                           </children>
                        </AnchorPane>
                    </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
