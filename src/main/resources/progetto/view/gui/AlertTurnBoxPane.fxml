<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="mainVBox" prefHeight="131.0" prefWidth="280.0" spacing="15.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="progetto.view.gui.AlertTurnBoxPaneController">
   <children>
      <HBox>
         <children>
            <Region HBox.hgrow="ALWAYS" />
            <Label text="E' il tuo turno!">
               <font>
                  <Font name="Imprint MT Shadow" size="25.0" />
               </font>
            </Label>
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <HBox>
         <children>
            <Region HBox.hgrow="ALWAYS" />
            <Button mnemonicParsing="false" onAction="#onOkButtonClicked" prefHeight="47.0" prefWidth="100.0" style="-fx-background-color: wheat; -fx-border-color: black;" text="Ok">
               <font>
                  <Font name="Imprint MT Shadow" size="25.0" />
               </font>
            </Button>
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
   </padding>
</VBox>
